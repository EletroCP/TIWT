{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "warn",
        "no-irregular-whitespace": "error",
        "indent": ["error", 2],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "camelcase": "warn",
        "complexity": ["error", 5],
        "no-var": "error",
        "max-len": ["error", 90],
        "comma-spacing": ["error", { "before": false, "after": true}],
        "object-curly-spacing": ["error", "always"]
    }
}
